{
  "expo": {
    "name": "Lumigram",
    "slug": "lumigram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00003C"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#00003C"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "firebaseApiKey": "AIzaSyCsKI7puA4z_qr1igjoJ0IxDxkd5mB84t4",
      "firebaseAuthDomain": "atlas-lumigram.firebaseapp.com",
      "firebaseProjectId": "atlas-lumigram",
      "firebaseStorageBucket": "atlas-lumigram.appspot.com",
      "firebaseMessagingSenderId": "978746231167",
      "firebaseAppId": "1:978746231167:web:4c45a17404beb23de6f24a",
      "firebaseMeasurementId": "G-41QMNJ754N"
    }
  }
}